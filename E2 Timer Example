@name Timer Example

if(first()){
    print("Timer *First* will be activated at if first statmenet")
    
    #here the 1000 indicates how long it takes for it to trigger, the units are in ms, 1000 is 1 second
    timer("First",1000)
}

if(clk("First")){
    #now timer will trigger after 1000 ms when it has loaded
    print("Timer triggered")
}
